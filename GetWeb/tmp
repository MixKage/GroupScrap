<script type="text/html" id="btn_enter">
	<!-- ko if: ($component.enterButton() === 'LOGIN_CONFLICT') -->
		<!-- ko if: rec().access.value() <= 3 -->
			<a href="#" class="btn btn-important" data-bind="openModal: {
						'name': 'modal-action',
						'params': {
							title: l10n.getTranslation('reenter_title'),
							text: l10n.getTranslation('reenter_desc'),
							actions: [
								{
									'text': l10n.getTranslation('common_continue'),
									'class': 'btn btn-important',
									'id': 'reenter'
								},
								{
									'text': l10n.getTranslation('common_cancel'),
									'class': 'btn',
									'id': 'cancel'
								}
							],
							selectVariant: function(result) {
								switch (result.id) {
									case 'reenter':
										modals.closeAll();
										reenter();
										break;
									case 'cancel':
										modals.closeAll();
										break;
								}
							}
						}
					}, translation: 'reenter'"></a>
		<!-- /ko -->

